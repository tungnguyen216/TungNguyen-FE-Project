{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": 0,
        "curly": 2,
        "eqeqeq": 2,
        "new-cap": 2,
        "camelcase": 2,
        "no-trailing-spaces": 2
    }
}
